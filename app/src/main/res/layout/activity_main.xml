<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ks.plugin.widget.launcher.MainActivity">

    <WebView
        android:id="@+id/vweb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <!--android:text="https://mbd.baidu.com/newspage/data/landingsuper?context=%7B%22nid%22%3A%22news_6077062882126362872%22%7D&amp;n_type=0&amp;p_from=1"-->

    <EditText
        android:id="@+id/vurl"
        android:text="http://news.sina.com.cn/china/xlxw/2018-03-08/doc-ifxpwyhw0567132.shtml"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/saveWeb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="保存整个网页" />

    <Button
        android:id="@+id/saveArticle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="保存文章"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.783"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.526" />

    <Button
        android:id="@+id/vtitlephoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="唱吧头像个性装扮"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="373dp" />
</LinearLayout>